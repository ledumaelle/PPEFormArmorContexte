<AnchorPane id="AnchorPane" prefHeight="568.0" prefWidth="464.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controleur.FXMLDescriptionSessionAcheveeController">
   <children>
      <Label layoutX="46.0" layoutY="25.0" text="Description d'une session achevée " textFill="#1306d0">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <Label layoutX="70.0" layoutY="100.0" text="Libellé : " />
      <Label layoutX="70.0" layoutY="130.0" text="Date : " />
      <Label layoutX="70.0" layoutY="190.0" text="Nombres d'inscrits : " />
      <Label layoutX="70.0" layoutY="250" text="Taux de remplissage : " />
      <Label layoutX="70.0" layoutY="160.0" text="Nombre de places : " />
      <Label layoutX="70.0" layoutY="220" text="Nombres d'abscents : " />
      <Label id="lblLibelle" fx:id="lblLibelle" layoutX="300.0" layoutY="100.0" />
      <Label id="lblDate" fx:id="lblDate" layoutX="300.0" layoutY="130.0" />
      <Label id="lblNbPlaces" fx:id="lblNbPlaces" layoutX="300.0" layoutY="160.0" />
