      <Label id="lblTauxRemplissage" fx:id="lblTauxRemplissage" layoutX="300.0" layoutY="250" />
      <Label layoutX="70.0" layoutY="370.0" text="Liste des participants : " />
      <TableView fx:id="tabParticipants" layoutX="220.0" layoutY="370.0" prefHeight="191.0" prefWidth="232.0">
        <columns>
          <TableColumn id="colEmploye" fx:id="colEmploye" prefWidth="114.0" text="Employé" />
          <TableColumn id="colHoraire" fx:id="colHoraire" prefWidth="94.0" text="Taux Horaire" />
        </columns>
      </TableView>
      <Label layoutX="70.0" layoutY="310.0" text="Chiffre d'affaires de la formation : " />
      <Label id="lblVente" fx:id="lblVente" layoutX="300.0" layoutY="310.0" />
