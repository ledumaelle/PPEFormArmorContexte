      <Label id="lblCoutRevient" fx:id="lblCoutRevient" layoutX="300.0" layoutY="280.0" />
      <Label layoutX="70.0" layoutY="340.0" text="Marge : " />
      <Label id="lblMarge" fx:id="lblMarge" layoutX="300.0" layoutY="340.0" />      
      <Label id="lblNombreAbsents" fx:id="lblNombreAbsents" layoutX="300" layoutY="220" />
      <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fitHeight="64" fitWidth="150" layoutX="14.0" layoutY="490.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/formationProfessionnelle.jpg" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
